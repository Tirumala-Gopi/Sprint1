package Account_and_customer_management;

public class CheckingAccount extends Account {

	private int nextCheckNumber;
	static int checkNumber=1;

	public int getNextCheckNumber() {
		return nextCheckNumber;
	}

	public void setNextCheckNumber(int nextCheckNumber) {
		this.nextCheckNumber = nextCheckNumber;
	}

	@Override
	public String toString() {
		return "CheckingAccount [nextCheckNumber=" + nextCheckNumber + "]";
	}

	public CheckingAccount() {
		super();
		this.nextCheckNumber = checkNumber;
		checkNumber=nextCheckNumber+1;
	}

	
	
	
}
