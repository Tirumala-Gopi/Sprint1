package dependenciesExercise2;

import java.util.ArrayList;

public class Runner {

	public static void main(String[] args){
	SmartDevice device1=new SmartDevice("apple",24.0);
	SmartDevice device2=new SmartDevice("samsung",33.0);
	
	System.out.println("smartdevice"+device1);
	System.out.println("smartdevice"+device2);
	
	
	Employee first=new Employee("ramesh",118);
	Employee second=new Employee("suresh",119);
	
	System.out.println("employee"+first);
	System.out.println("employee"+second);
	
	first.usedevice(device1);
	second.usedevice(device2);
	
	

	PowerUnit ps=new PowerUnit(2.4);
	SmartDevice device3=new SmartDevice("lenovo",ps.getWatts());
	
	System.out.println("smartdevice"+device3);
	
	ArrayList<SmartDevice> devices=new ArrayList<>();
	devices.add(device1);
	devices.add(device2);
	
	OfficeRoom room1=new OfficeRoom("USA",devices);
	
	System.out.println("officeroom"+room1);
	ArrayList<OfficeRoom> rooms=new ArrayList<>();
	rooms.add(room1);
	SmartOffice room2=new SmartOffice("japan",rooms);
	
	
	System.out.println();
	
}
}